  "bugs": {
    "url": "https://github.com/yakubique/npm-versions/issues"
  },
  "homepage": "https://github.com/yakubique/npm-versions#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@actions/http-client": "^2.2.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.1",
    "@types/semver": "^7.5.6",
    "@vercel/ncc": "^0.38.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
